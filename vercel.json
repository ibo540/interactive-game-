{
  "version": 2,
  "builds": [
    {
      "src": "DarkAlchemyProject/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/DarkAlchemyProject/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/DarkAlchemyProject/$1"
    }
  ]
}
